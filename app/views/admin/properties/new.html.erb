<div class="wrapper">
		<!-- Sidebar Holder -->
		<%=render 'partials/admin_sidebar'%>
		<!-- Page Content Holder -->
		<div id="content">
			<%=render 'partials/admin_header'%>
			<div class="row">
				<div class="col-sm-4 col-md-6 col-xs-12">
					<p>Add a New Property</p>
				</div>
			</div>
			<% if flash.present?%>
				<div class="row">
					<div class="col-sm-4 col-md-6 col-xs-12">
						<% if flash[:notice] %>
							<p style="color:black;"><%=flash[:notice]%></p>
						<%elsif flash[:alert]%>
							<p><%=flash[:alert].html_safe%></p>
						<%end%>
					</div>
				</div>
			<%end%>
			<div class="line"></div>
			<div class="row">
				<div class="col-sm-12">
					<%= form_for @property, url: properties_path, method: 'post',html:{class:'form-class'} do |f| %>
						<div class="form-row">
							<div class="form-group col-md-6">
								<label for="inputEmail4" class="col-form-label">Name</label>
								<%= f.text_field :name, class:'form-control',placeholder: 'display name'%>
							</div>
							<div class="form-group col-md-6">
								<label for="inputPassword4" class="col-form-label">Display Name</label>
								<%= f.text_field :display_name, class:'form-control',placeholder: 'display name'%>
							</div>
						</div>
						<div class="form-row" >
							<div class="form-group col-sm-6" style="clear:both;">
								<label for="inputEmail4" class="col-form-label">Properties</label>
								<select style="width:100%;"class="js-example-basic-multiple " name="states[]" multiple="multiple">
								  <option value="AL">Alabama</option>
								  <option value="WY">Wyoming</option>
								</select>
							</div>
						</div>

						<div class="form-row" style="clear:both;">
							<div class="form-group col-sm-4 col-md-6 col-sm-12">
								<%= button_tag(type: 'submit', class: 'btn btn-success') do %>
									<span><i class='glyphicon glyphicon-ok'></i>&nbsp;Create</span>
								<% end %>
							</div>
						</div>
					<%end%>
				</div>
			</div>

		</div>
</div>
